<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logoo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kebunpintar"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".JadwalActivity"
            android:exported="false"
            android:theme="@style/Theme.Kebunpintar.NoActionBar"/>

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity
            android:name=".GardenMonitorActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.Kebunpintar.NoActionBar" />
        <activity
            android:name=".GardenInfoActivity"
            android:exported="false"
            android:parentActivityName=".GardenMonitorActivity"
            android:theme="@style/Theme.Kebunpintar.NoActionBar" />
        <activity
            android:name=".UserActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.Kebunpintar.NoActionBar"/>
        <activity
            android:name=".RegisterActivity"
            android:exported="false"
            android:parentActivityName=".LoginActivity"
            android:theme="@style/Theme.Kebunpintar.NoActionBarlogindkk" />
        <activity
            android:name=".LoginActivity"
            android:exported="true"
            android:theme="@style/Theme.Kebunpintar.NoActionBarlogindkk">

        </activity>
        <activity
            android:name=".SplashScreen"
            android:exported="true"
            android:theme="@style/Theme.Kebunpintar.NoActionBarWhite">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Kebunpintar.NoActionBarWhite">

        </activity>
    </application>

</manifest>